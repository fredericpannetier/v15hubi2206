<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_partner_customer" model="ir.ui.view">
            <field name="name">partner.customer</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field eval="90" name="priority"/>
            <field name="arch" type="xml">
            
                <xpath expr="//form" position="inside">
                    <link rel="stylesheet" href="/hubi/static/src/css/style.css"/>
                </xpath>
            
                <xpath expr="//field[@name='title']" position="replace"/>
                
                <div name="button_box" position="inside">
                    <button type="object" class="oe_stat_button" icon="fa-list" name="action_view_histo_invoice" context="{'default_partner_id': active_id}" attrs="{'invisible': [('di_total_archive', '=', 0)]}">
                        <div class="o_form_field o_stat_info">
                            <span class="o_stat_value">
                                
                                <field name="di_total_archive" />
                            </span>
                            <span class="o_stat_text">Total Archive</span>
                        </div>
                    </button>
                
                    <!-->
                    icon="fa-pencil-square-o"
                    <button type="object" class="oe_stat_button" name="open_action_followup" icon="fa-list" attrs="{'invisible': [('di_total_due', '=', 0)]}">
                        <div class="o_field_widget o_stat_info">
                            <span class="o_stat_value"><field name="di_total_due" widget='monetary' options="{'currency_field': 'currency_id'}"/></span>
                            <span class="o_stat_text">Due</span>
                        </div>
                    </button>
                    -->
                </div>


                <xpath expr="//field[@name='parent_id']" position="after">
                    <field name="company_name" attrs="{'invisible': ['|', '|', ('company_name', '=', True), ('company_name', '!=', ''), ('is_company', '=', False)]}"/>
                    <field name="title"/>
                </xpath>
            
                <xpath expr="//field[@name='company_type']" position="after">
                    <label for="di_is_establishment" string="Is an establishment" />  
                    <field name="di_is_establishment" string="Is an establishment" widget="boolean" />  
                </xpath>
               
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="di_code_ean" />
                </xpath>
  <!--
                <xpath expr="//field[@name='category_id']" position="after">
                    <group attrs="{'invisible': [('is_company','=',True),('customer_rank','=',0),('supplier_rank','=',0)]}">
                        <field name="customer_rank" string="Customer Invoices" />   
                        <field name="supplier_rank" string="Vendor Bills" />
                        
                        
                    </group>
                </xpath>
                -->
                        <!-->
                        <field name="di_is_customer" string="Customer Invoices" widget="boolean" />   
                        <field name="di_is_supplier" string="Vendor Bills" widget="boolean"/>
                        -->

                <xpath expr="//page[@name='internal_notes']" position="before">
                    <page name="characteristic" string="Characteristic" >
                        <group name="characteristic_row_1">
                            <group string="Various" name="carac_various" attrs="{'invisible': [('di_is_establishment','=',True)]}">
                                <field name="di_sender_establishment" domain="[('di_is_establishment','=',True)]" options="{'no_create': True}" />
                                <field name="di_excluded_packaging" />
                            </group>
                            <group string="Information" name="information" attrs="{'invisible': [('di_is_establishment', '=', False)]}">
                                
                                <field name="di_asset" />
                                <field name="di_rcs" />
                                <field name="di_naf" />
                                <field name="di_health_number" />
                                
                            </group>
                        </group>
                    </page>
                </xpath>

                <xpath expr="//page[@name='characteristic']" position="after">
                    <page name="management" string="Management" attrs="{'invisible': [('di_is_establishment','=',True)]}">
                        <group name="management_row_1">
<!-->                        
                            <group string="Various" name="management_various">
                                 <field name="di_invoice_grouping" />
                                <field name="di_periodicity_invoice" />
                            </group>
-->                            
                            <group string="Discount" name="discount">
                                <field name="di_discount_invoice" />
                                <field name="di_discount_ca" />
                                <field name="di_discount_period_ca" />
                                <field name="di_discount_description" />
                            </group>
                        </group>
                    </page>
                </xpath>

                <xpath expr="//page[@name='management']" position="after">
                    <page name="impression" string="Impression" attrs="{'invisible': [('di_is_establishment','=',True)]}">
                        <group name="impression_row">
                            <group name="impression_row_1">
                                <field name="di_product_grouping" />
                                <field name="di_valued_delivery" />
                                
                            </group>
                        </group>
                        <group name="impression_row_3">
                            <field name="di_bottom_message_invoice" />
                            <field name="di_bottom_message_delivery" />
                        </group>
                    </page>
                </xpath>

                <xpath expr="//page[@name='impression']" position="after">
                    <page name="etiquette" string="Etiquette" >
                        <group name="etiquette_row" attrs="{'invisible': [('di_is_establishment','=',True)]}">
                            <group name="etiquette_row_1">
                                <field name="di_customer_name_etiq" />
                                <field name="di_customer_city_etiq" />
                                <field name="di_etiq_model_id" />
                                <field name="di_etiq_printer" />
                                <div>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_magenta"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FF00FF')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_blue"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#0000FF')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_yellow"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FFFF00')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_red"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FF0000')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_green"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#008000')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_brown"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#D2691E')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_white"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FFFFFF')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_grey"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#CCCCCC')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_pink"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FFC0CB')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_white"  attrs="{'invisible': [('di_customer_color_etiq', '!=', False)]}"/>
                                </div>
                                <field name="di_customer_color_etiq" nolabel="1" />
                            </group>
                            <group name="etiquette_row_2">
                                <!--<field name="di_code_ean" />-->
                                <field name="di_automatic_batch"  />
                                <field name="di_dlc" />
                                <field name="di_dlc_number_day"/>

                            </group>
                        </group>
                        
                        
                        <!--<group name="etiquette_row_establ" attrs="{'invisible': ['|',('customer_rank','!=',0),('supplier_rank','!=',0)]}">-->
                        <group name="etiquette_row_establ" attrs="{'invisible': [('di_is_establishment', '=', False)]}">
                            <group name="etiquette_row">
                                <field name="di_company_name_etiq" />
                                <field name="di_company_city_etiq" /> 
                                <field name="di_etiq_mention" /> 
                            </group>  
                            <group name="etiquette_row_2">
                                <field name="di_code_ean128" />   
                                <field name="di_compteur_ean128" />
                            </group>
                        </group>
                        
                    </page>
                </xpath>

                <xpath expr="//page[@name='etiquette']" position="after">
                    <page name="statistics" string="Statistics">
                        <group name="statistics_group">
                            <field name="di_statistics_alpha_1"/>
                            <field name="di_statistics_alpha_2"/>
                            <field name="di_statistics_alpha_3"/>
                            <field name="di_statistics_alpha_4"/>
                            <field name="di_statistics_alpha_5"/>
                        </group>
                    </page>
                </xpath>
<!-->
                <xpath expr="//page[@name='etiquette']" position="after">
                    <page name="EDI" string="EDI" attrs="{'invisible': [('di_is_establishment','=',True)]}">
                        <group name="EDI_invoice" string="EDI Invoice">
                            <group name="EDI_invoice_row_1">
                                <field name="di_edi_invoice" />
                                <field name="di_edi_transport_recipient" />
                            </group>
                            <group name="EDI_invoice_row_2" attrs="{'invisible': [('di_edi_invoice', '=', False)]}">
                                <field name="di_edi_invoice_prod" />
                                <field name="di_order_code_ean" />
                                <field name="di_order_name" />
                            </group>
                        </group>
                    </page>
                </xpath>
-->
                <group name="purchase" position="inside">
                    <field name="di_amount_com_kg" attrs="{'invisible': [('di_is_establishment','=',True)]}"/> 
                </group>
                
                <!-->
                <xpath expr="//page[@name='sales_purchases']/group/group[@name='misc']/field[@name='industry_id']" position="after">
                    <field name="vat" string="VAT" placeholder="e.g. FR0477472701"/>
                </xpath>
                -->
            </field>
        </record>
        
